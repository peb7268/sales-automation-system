{
  "name": "automated-agentic-sales-team",
  "version": "1.0.0",
  "description": "AI-powered sales automation system with geographic prospecting, pitch creation, and multi-channel outreach",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && npm run test:open-coverage",
    "test:ui": "majestic --port 4000",
    "test:ui-open": "npm run test:ui && open http://localhost:4000",
    "test:full": "npm run test:coverage && npm run test:open-report",
    "test:open-report": "open test-report.html",
    "test:open-coverage": "open coverage/lcov-report/index.html",
    "test:ci": "jest --coverage --reporters=default --reporters=jest-junit",
    "test:ui-auto": "./test-ui.sh",
    "test:dev": "npm run test:ui-auto",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist",
    "setup": "npm install && npm run build",
    "setup:obsidian": "node scripts/setup/obsidian-setup.js",
    "setup:env": "cp .env.example .env",
    "agents:start": "node dist/orchestration/agent-manager.js",
    "agents:stop": "pkill -f agent-manager",
    "prospect": "tsx src/commands/prospect-businesses-mastra.ts",
    "prospect:legacy": "tsx src/commands/prospect-businesses.ts",
    "add-business": "tsx src/commands/add-business.ts",
    "add-prospect": "tsx src/commands/add-prospect.ts",
    "add-prospect-enhanced": "tsx src/commands/add-prospect-enhanced.ts",
    "prospect:add": "npm run add-prospect",
    "prospect:enhanced": "npm run add-prospect-enhanced",
    "prospect:retry": "npm run add-prospect-enhanced -- --retry",
    "prospect:status": "tsx src/commands/prospect-status.ts",
    "pipeline:status": "node dist/orchestration/pipeline-status.js",
    "sync-kanban": "tsx scripts/sync-kanban.ts",
    "kanban:sync": "npm run sync-kanban",
    "kanban:monitor": "tsx scripts/kanban-monitor.ts",
    "update-analytics": "tsx scripts/update-analytics.ts",
    "analytics:update": "npm run update-analytics",
    "analytics:daily": "npm run update-analytics",
    "test-analytics": "tsx scripts/test-analytics-integration.ts",
    "analytics:test": "npm run test-analytics",
    "migrate-prospects": "tsx scripts/migrate-prospects-to-folders.ts",
    "generate-pitches": "tsx src/commands/generate-pitches-mastra.ts",
    "generate-pitches:legacy": "tsx src/commands/generate-pitches.ts",
    "pitches:generate": "npm run generate-pitches",
    "pitches:legacy": "npm run generate-pitches:legacy",
    "pitches:all": "npm run generate-pitches -- --all --evaluate",
    "pitches:all:legacy": "npm run generate-pitches:legacy -- --all",
    "pitches:verbose": "npm run generate-pitches -- --all --verbose",
    "fix-dataview": "tsx scripts/fix-dataview-queries.ts",
    "clean-dashboard": "tsx scripts/create-clean-dashboard.ts",
    "task-manager": "tsx src/commands/task-manager-cli.ts",
    "tasks:start": "npm run task-manager start",
    "tasks:status": "npm run task-manager status",
    "tasks:list": "npm run task-manager list",
    "mastra": "tsx src/commands/mastra-demo.ts",
    "mastra:init": "npm run mastra init",
    "mastra:prospect": "npm run mastra prospect",
    "mastra:pitch": "npm run mastra pitch",
    "mastra:workflow": "npm run mastra workflow",
    "mastra:evaluate": "npm run mastra evaluate",
    "mastra:status": "npm run mastra status",
    "verify:migration": "tsx scripts/verify-mastra-migration.ts",
    "install:global": "bash bin/install-global-commands.sh",
    "uninstall:global": "bash bin/uninstall-global-commands.sh",
    "postinstall": "echo 'Run npm run install:global to install global commands'"
  },
  "keywords": [
    "sales-automation",
    "ai-agents",
    "prospecting",
    "obsidian",
    "crm",
    "digital-marketing"
  ],
  "author": "Mile High Marketing",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/openai": "^2.0.0",
    "@anthropic-ai/sdk": "^0.24.3",
    "@googlemaps/google-maps-services-js": "^3.3.42",
    "@mastra/core": "^0.12.1",
    "@sendgrid/mail": "^8.1.3",
    "@types/amqplib": "^0.10.7",
    "@types/ws": "^8.18.1",
    "amqplib": "^0.10.8",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "joi": "^17.13.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "puppeteer": "^22.8.0",
    "rate-limiter-flexible": "^5.0.3",
    "twilio": "^5.0.4",
    "uuid": "^9.0.1",
    "winston": "^3.13.0",
    "ws": "^8.18.3",
    "yaml": "^2.4.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@jest/core": "^30.1.0",
    "@types/cheerio": "^0.22.35",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/node": "^20.12.12",
    "@types/nodemailer": "^6.4.15",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "majestic": "^1.8.1",
    "nodemon": "^3.1.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.10.5",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mile-high-marketing/automated-agentic-sales-team.git"
  },
  "bugs": {
    "url": "https://github.com/mile-high-marketing/automated-agentic-sales-team/issues"
  },
  "homepage": "https://github.com/mile-high-marketing/automated-agentic-sales-team#readme"
}
