# Obsidian Configuration
OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault

# API Keys - External Services
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here
YELLOW_PAGES_API_KEY=your_yellow_pages_api_key_here
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
PERPLEXITY_API_KEY=your_perplexity_api_key_here

# AI Services
ANTHROPIC_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Business System Integrations
FRESHBOOKS_API_KEY=your_freshbooks_api_key_here
FRESHBOOKS_ACCOUNT_ID=your_freshbooks_account_id_here
LINEAR_API_KEY=your_linear_api_key_here

# Email Service Provider
SENDGRID_API_KEY=your_sendgrid_api_key_here
# Alternative: MAILGUN_API_KEY=your_mailgun_api_key_here

# Voice AI Platform
VOICE_AI_API_KEY=your_voice_ai_platform_key_here
VOICE_AI_BASE_URL=https://api.voice-platform.com

# Application Configuration
NODE_ENV=development
LOG_LEVEL=info
PORT=3000

# Database Configuration (if using external DB)
DATABASE_URL=your_database_connection_string_here

# Security
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Pipeline Configuration
DEFAULT_QUALIFICATION_THRESHOLD=50
MAX_PROSPECTS_PER_DAY=100
MAX_CONCURRENT_CALLS=5
EMAIL_RATE_LIMIT_PER_HOUR=50

# Compliance Settings
ENABLE_TCPA_COMPLIANCE=true
ENABLE_CAN_SPAM_COMPLIANCE=true
DEFAULT_TIMEZONE=America/Denver